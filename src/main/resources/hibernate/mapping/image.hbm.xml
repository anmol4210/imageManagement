<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping>
	<class table="imagerecords" lazy="false" name="com.nagarro.model.Image">
	<id name="id" type="int" column="id">
		<generator class="increment"></generator>
	</id>
		<property name="username" type="string" column="username"></property>
		<property name="imageName" type="string" column="image_name"></property>
		<property name="image" type="byte[]" column="image"></property>
	</class>
</hibernate-mapping>